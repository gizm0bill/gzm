{
  "name": "@gzm/ng-rest-client",
  "version": "2.0.0",
  "peerDependencies": {
    "@angular/common": "15 - 19",
    "@angular/core": "15 - 19",
    "typescript": "4 - 5",
    "reflect-metadata": "1 - 2"
  },
  "description": "Angular REST Client utility library",
  "scripts": {
    "build:schematics": "tsc -p tsconfig.schematics.json && cp schematics/*.json ../../dist/ng-rest-client/schematics/"
  },
  "ng-update": {
    "migrations": "./schematics/migration.json"
  },
  "ng-add": {
    "save": "dependencies"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gizm0bill/gzm.git",
    "directory": "libs/ng-rest-client"
  },
  "keywords": [
    "Angular",
    "REST"
  ],
  "author": "gizm0bill@gmail.com",
  "license": "MIT",
  "schematics": "./schematics/collection.json",
  "readme": "README.md"
}
